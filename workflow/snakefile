
rule all:
    input:
        'data/raw/df_raw_test.zip',
        'data/clean/df_clean.zip',
        'data/df_work.zip'

rule clean:
    input:
        'data/raw/df_raw_test.zip'
    output:
        'data/clean/df_clean.zip'
    shell:
        'python -m src.data.clean_df {input} {output}'

rule features:
    input:
        'data/clean/df_clean_test.zip'
    output:
        'data/df_work.zip'
    shell:
        'python -m src.features.make_features {input} {output}'